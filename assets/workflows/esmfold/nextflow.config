params {
    max_records_per_partition = 1
    model_parameters = "s3://{{S3_BUCKET_NAME}}/ref-data/esmfold_parameters/facebook/esmfold_v1/model.tar"
}

process {
    withLabel: esmfold { 
        container = "{{pytorch:latest}}"
        cpus = 8
        memory = '32 GB'
        accelerator = "1, type: 'nvidia-tesla-a10g'"
        publishDir = "/mnt/workflow/pubdir"
        }
}